<% error = @website_collection.errors.include?(:website_id) %>
<fieldset class='control-group <%= error ? 'has-error' : ''%>'>
  <%= f.label :被收藏网站, :class => 'control-label' %>
  <div class='controls'>
    <%= f.text_field :website_id, :class => 'form-control input-xlarge input-with-feedback', :autofocus => true %>
    
  </div>
</fieldset>
<% error = @website_collection.errors.include?(:account_id) %>
<fieldset class='control-group <%= error ? 'has-error' : ''%>'>
  <%= f.label :收藏者, :class => 'control-label' %>
  <div class='controls'>
    <%= f.text_field :account_id, :class => 'form-control input-xlarge input-with-feedback' %>
    
  </div>
</fieldset>


<div class="form-actions">
  <%= f.submit pat(:save), :class => 'btn btn-primary' %>
  &nbsp;
  <%= f.submit pat(:save_and_continue), :class => 'btn btn-info', :name => 'save_and_continue' %>
  &nbsp;
  <%= link_to pat(:cancel), url(:website_collections, :index), :class => 'btn btn-default' %>
</div>