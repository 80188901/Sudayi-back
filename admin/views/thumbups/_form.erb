<% error = @thumbup.errors.include?(:account_id) %>
<fieldset class='control-group <%= error ? 'has-error' : ''%>'>
  <%= f.label :称赞者, :class => 'control-label' %>
  <div class='controls'>
    <%= f.text_field :account_id, :class => 'form-control input-xlarge input-with-feedback', :autofocus => true %>
  </div>
</fieldset>
<% error = @thumbup.errors.include?(:post_id) %>
<fieldset class='control-group <%= error ? 'has-error' : ''%>'>
  <%= f.label :分享内容编号, :class => 'control-label' %>
  <div class='controls'>
    <%= f.text_field :post_id, :class => 'form-control input-xlarge input-with-feedback' %>
    
  </div>
</fieldset>
<% error = @thumbup.errors.include?(:product_id) %>
<fieldset class='control-group <%= error ? 'has-error' : ''%>'>
  <%= f.label :商品编号, :class => 'control-label' %>
  <div class='controls'>
    <%= f.text_field :product_id, :class => 'form-control input-xlarge input-with-feedback' %>
  </div>
</fieldset>
<% error = @thumbup.errors.include?(:image_item_id) %>
<fieldset class='control-group <%= error ? 'has-error' : ''%>'>
  <%= f.label :图片编号, :class => 'control-label' %>
  <div class='controls'>
    <%= f.text_field :image_item_id, :class => 'form-control input-xlarge input-with-feedback' %>
    
  </div>
</fieldset>


<div class="form-actions">
  <%= f.submit pat(:save), :class => 'btn btn-primary' %>
  &nbsp;
  <%= f.submit pat(:save_and_continue), :class => 'btn btn-info', :name => 'save_and_continue' %>
  &nbsp;
  <%= link_to pat(:cancel), url(:thumbups, :index), :class => 'btn btn-default' %>
</div>