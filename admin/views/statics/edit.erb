<ul class="nav nav-tabs">
  <li> <%= link_to tag_icon(:list, pat(:list)), url(:statics, :index) %></li>
  <li> <%= link_to tag_icon(:plus, pat(:new)), url(:statics, :new) %></li>
  <li class="active"> <%= link_to tag_icon(:edit, pat(:edit)), url(:statics, :edit, :id => @static.id) %></li>
</ul>
<div class="tabs-content">
  <% form_for :static, url(:statics, :update, :id => @static.id), :method => :put, :class => 'form-horizontal' do |f| %>
    <%= partial 'statics/form', :locals => { :f => f } %>
  <% end %>
</div>
